import java.util.Scanner;

class Main {

  public static void main(String args[]) {
    Scanner in = new Scanner(System.in);
    System.out.println("Enter number below:");
    int num= in.nextInt();
    if(num%2==0){
    System.out.println("even number");
    }
    else{
      System.out.println("Odd number");
    }
  }
}
