import java.util.*;

class Main {
  public static void main(String[] args) {

    Scanner input = new Scanner(System.in);
    System.out.println("Hello! enter username below:");

    String username2 = input.nextLine();
    System.out.println("Hello! enter password below:");
    String password2 = input.nextLine();

    String username1 = "thebestname";
    String password1 = "thebestword";

    if (username2.equals(username1)) {
      System.out.println("Welcome");
    } else {
      System.out.println("unregistered user");
    }

    if (password2.equals(password1)) {
      System.out.println(username2);
    } else {
      System.out.println("password incorrect");
    }
  }
}
